library cherrymxBlack;

uses
  System.SysUtils,
  System.Classes, mmSystem;

{$R *.res}
{$R cherrymx-black-abs.res}

procedure playClick (button: string);
begin
   button:=copy(button,2,length(button)-2);
   if fileexists(button) then
   sndPlaySound(pchar(button), SND_RESOURCE + SND_ASYNC + SND_NODEFAULT)
   else
   sndPlaySound(pchar('remain'), SND_RESOURCE + SND_ASYNC + SND_NODEFAULT)

end;

exports playClick;
end.
