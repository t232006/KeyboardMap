unit saver;

interface
uses KeyboardUnit;
procedure savetext(filename, sometext:string);
procedure savemap(filename: string; map:TKeyboardMap);

implementation
procedure savetext(filename, sometext:string);
var f:textfile;
begin
  assignfile(f, filename);
  try
    append(f);
    writeln(f,sometext);
  except
    rewrite(f);
  end;

  closefile(f);

end;

procedure savemap(filename: string; map:TKeyboardMap);
begin


end;

end.
